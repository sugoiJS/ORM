{
  "name": "@sugoi/orm",
  "version": "2.4.0",
  "description": "SugoiJS ORM",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --config=jest.config.js --coverage --detectOpenHandles",
    "coverge": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "docs": "..\\node_modules\\.bin\\typedoc --out ..\\dist\\docs\\orm --json dist\\docs\\main.json --ignoreCompilerErrors --excludeNotExported --excludeProtected --excludePrivate --readme .\\README.md --mode file --hideGenerator --externalPattern node_modules/**/* --excludeExternals ",
    "update:patch": "npm run build && npm version patch && npm publish && npm run docs",
    "update:minor": "npm run build && npm version minor && npm publish && npm run docs",
    "update:major": "npm run build && npm version major && npm publish && npm run docs",
    "build": "rmdir /S dist && tsc"
  },
  "keywords": [
    "orm",
    "schemas",
    "decorators",
    "annotations",
    "@sugoi",
    "@sugoijs",
    "sugoi",
    "sugoijs",
    "utils",
    "typescript"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "Orel Balilti",
  "license": "MIT",
  "homepage": "http://sugoijs.com",
  "bugs": {
    "url": "https://github.com/sugoiJS/ORM/issues",
    "email": "oba.sugoi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sugoiJS/ORM"
  },
  "dependencies": {
    "@sugoi/core": "2.1.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./dist/coverage"
  }
}
